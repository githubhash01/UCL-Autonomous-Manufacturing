<?xml version="1.0"?>
<robot name="x500">

  <!-- Base frame -->
  <link name="base_link">
    <inertial>
      <mass value="1.5"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.02" iyy="0.02" izz="0.04" ixy="0" ixz="0" iyz="0"/>
    </inertial>

    <!-- Use your uploaded .dae mesh -->
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="5010Base.dae" scale="1 1 1"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="5010Base.dae" scale="1 1 1"/>
      </geometry>
    </collision>
  </link>

  <!-- Rotor 0 -->
  <link name="rotor_0">
    <visual>
      <origin xyz="0.15 0.15 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.01" radius="0.05"/>
      </geometry>
      <material name="black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
  </link>

  <joint name="rotor_0_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rotor_0"/>
    <origin xyz="0.15 0.15 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- Rotor 1 -->
  <link name="rotor_1">
    <visual>
      <origin xyz="-0.15 0.15 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.01" radius="0.05"/>
      </geometry>
    </visual>
  </link>

  <joint name="rotor_1_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rotor_1"/>
    <origin xyz="-0.15 0.15 0" rpy="0 0 0"/>
    <axis xyz="0 0 -1"/>
  </joint>

  <!-- Rotor 2 -->
  <link name="rotor_2">
    <visual>
      <origin xyz="-0.15 -0.15 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.01" radius="0.05"/>
      </geometry>
    </visual>
  </link>

  <joint name="rotor_2_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rotor_2"/>
    <origin xyz="-0.15 -0.15 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- Rotor 3 -->
  <link name="rotor_3">
    <visual>
      <origin xyz="0.15 -0.15 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.01" radius="0.05"/>
      </geometry>
    </visual>
  </link>

  <joint name="rotor_3_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rotor_3"/>
    <origin xyz="0.15 -0.15 0" rpy="0 0 0"/>
    <axis xyz="0 0 -1"/>
  </joint>

</robot>
